// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario {
  ID_Usuario Int @id @default(autoincrement())
  Email String @unique
  Nome  String
  Senha String
  Plano String
  Foto  String?
}